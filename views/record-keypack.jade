doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    title Record Random Keypack Reward - Prime Hunter

    link(href="/public/css/bootstrap-3.3.2.min.css", rel="stylesheet")
    link(href="/public/css/prime.css", rel="stylesheet")

    if isprod
      script(src="/public/js/ga.js")
  body
    
    include navbar.jade
    
    div.jumbotron
      div.container
        div.row
          div.col-lg-8.col-md-8.col-sm-8
            h1 Record Void keypack results
            p What keys did you receive from your random key pack?
    
    div.container
      div.row
        div.col-lg-4.col-md-4.col-sm-4.random-key-chooser
          h2 Key 1
          p
            strong TIER
          div.btn-group.key1-tier
            button(type="button" data-tier="1" data-key="1").btn.btn-default 1
            button(type="button" data-tier="2" data-key="1").btn.btn-default 2
            button(type="button" data-tier="3" data-key="1").btn.btn-default 3
            button(type="button" data-tier="4" data-key="1").btn.btn-default 4
          p
            br
            strong MISSION
          div.btn-group.key1-mission
            button(type="button" data-key="1" data-mission="CAP").btn.btn-default CAP
            button(type="button" data-key="1" data-mission="DEF").btn.btn-default DEF
            button(type="button" data-key="1" data-mission="EXT").btn.btn-default EXT
            button(type="button" data-key="1" data-mission="INT").btn.btn-default.disabled INT
            button(type="button" data-key="1" data-mission="MD").btn.btn-default MD
            button(type="button" data-key="1" data-mission="SAB").btn.btn-default SAB
            button(type="button" data-key="1" data-mission="SUR").btn.btn-default SUR
            
        div.col-lg-4.col-md-4.col-sm-4.random-key-chooser
          h2 Key 2
          p
            strong TIER
          div.btn-group.key2-tier
            button(type="button" data-tier="1" data-key="2").btn.btn-default 1
            button(type="button" data-tier="2" data-key="2").btn.btn-default 2
            button(type="button" data-tier="3" data-key="2").btn.btn-default 3
            button(type="button" data-tier="4" data-key="2").btn.btn-default 4
          p
            br
            strong MISSION
          div.btn-group.key2-mission
            button(type="button" data-key="2" data-mission="CAP").btn.btn-default CAP
            button(type="button" data-key="2" data-mission="DEF").btn.btn-default DEF
            button(type="button" data-key="2" data-mission="EXT").btn.btn-default EXT
            button(type="button" data-key="2" data-mission="INT").btn.btn-default.disabled INT
            button(type="button" data-key="2" data-mission="MD").btn.btn-default MD
            button(type="button" data-key="2" data-mission="SAB").btn.btn-default SAB
            button(type="button" data-key="2" data-mission="SUR").btn.btn-default SUR
            
        div.col-lg-4.col-md-4.col-sm-4.random-key-chooser
          h2 Key 3
          p
            strong TIER
          div.btn-group.key3-tier
            button(type="button" data-tier="1" data-key="3").btn.btn-default 1
            button(type="button" data-tier="2" data-key="3").btn.btn-default 2
            button(type="button" data-tier="3" data-key="3").btn.btn-default 3
            button(type="button" data-tier="4" data-key="3").btn.btn-default 4
          p
            br
            strong MISSION
          div.btn-group.key3-mission
            button(type="button" data-key="3" data-mission="CAP").btn.btn-default CAP
            button(type="button" data-key="3" data-mission="DEF").btn.btn-default DEF
            button(type="button" data-key="3" data-mission="EXT").btn.btn-default EXT
            button(type="button" data-key="3" data-mission="INT").btn.btn-default.disabled INT
            button(type="button" data-key="3" data-mission="MD").btn.btn-default MD
            button(type="button" data-key="3" data-mission="SAB").btn.btn-default SAB
            button(type="button" data-key="3" data-mission="SUR").btn.btn-default SUR
          
        
      div#save-prompt.hidden
        div.row
          div.col-lg-12.col-md-12.col-sm-12
            p.center-text
              strong Choose Action
        div.row.reward-action
          div.col-lg-3.col-md-3.col-sm-3
            button#save-keypack.btn.btn-success.btn-lg.btn-block Save Key Pack
          div.col-lg-3.col-md-3.col-sm-3
            button#reset-keypack.btn.btn-danger.btn-lg.btn-block Reset
        div.row
          div.col-lg-12.col-md-12.col-sm-12.action-result
              
    
    include footer.jade
    
    script(src="/public/js/jquery-2.1.3.min.js")
    script(src="/public/js/bootstrap-3.3.2.min.js")
    script(src="/public/js/jquery.toaster.js")
    script(src="/public/js/record-keypack.js")
    script(src="/public/js/feedback.js")