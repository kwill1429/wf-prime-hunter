doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    title Void Key Pack Drop Rates - Prime Hunter

    link(href="/public/css/bootstrap-3.3.2.min.css", rel="stylesheet")
    link(href="/public/css/prime.css", rel="stylesheet")
    
    if isprod
      script(src="/public/js/ga.js")
  body
    
    include navbar.jade
    
    div.jumbotron
      div.container
        div.row
          div.col-lg-8.col-md-8.col-sm-12
            h1 Void Key Pack Drop Rates
            p Calculated drop rates.
    
    div.container
      div.row
        div.col-lg-6.col-md-6.col-sm-12
          h3.keypack-heading Key drop chance by Tiers
          div.row
            div.col-lg-6.col-md-6.col-sm-6
              canvas(width="200" height="200")#tier-canvas
            div.col-lg-6.col-md-6.col-sm-6#keypack-by-tiers
              div.tier-legend-holder
        div.col-lg-6.col-md-6.col-sm-12
          h3.keypack-heading Key drop chance by Missions
          div.row
            div.col-lg-6.col-md-6.col-sm-6
              canvas(width="200" height="200")#mission-canvas
            div.col-lg-6.col-md-6.col-sm-6#keypack-by-missions
              div.mission-legend-holder
              
    include footer.jade
    
    script(src="/public/js/jquery-2.1.3.min.js")
    script(src="/public/js/jquery.toaster.js")
    script(src="/public/js/bootstrap-3.3.2.min.js")
    script(src="/public/js/chart-1.0.1.min.js")
    script(src="/public/js/droprates-keypack.js")
    script(src="/public/js/feedback.js")
    